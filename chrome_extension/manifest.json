{
  "manifest_version": 2,

  "name": "Map Liberator",
  "description": "Map extension for freely choose between map engines",
  "version": "1.0",

  "content_scripts": [ 
    {
      "js": [ "jquery-1.9.1.min.js", "jquery-ui-1.10.2.custom.min.js", "googleEngine.js", "eventsRegistrator.js" ],
      "css": ["css/south-street/jquery-ui-1.10.2.custom.min.css"],
      "run_at": "document_end",
      "matches":["http://*/*", "https://*/*"]
    } 
  ],

  "permissions": [
    "http://*/*", "https://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  }
}